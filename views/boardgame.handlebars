<main>

  
  {{#each game}}
  <table>

    <tr>
      <th colspan="2">
        {{this.game_name}}
      </th>
    </tr>

    <tr>
      <td>
        Minimum # Of Players
      </td>
      <td>
        {{this.min_num_player}}
      </td>
    </tr>

    <tr> 
      <td>
        Maximum # Of Players
      </td>
      <td>
        {{this.max_num_player}}
      </td>
    </tr>

    <tr>
      <td>
        Rating
      </td>
      <td>
        {{this.rating}}
      </td>
    </tr>

    <tr>
      <td>
        Year Created
      </td>
      <td>
        {{this.year_created}}
      </td>
    </tr>

  </table>
  {{/each}}

  
</main>
